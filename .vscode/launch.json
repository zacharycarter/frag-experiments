  "version": "0.2.0",
  "configurations": [
    {
      "name": "(Windows) Launch",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/frag.exe",
      "args": ["-r", "..\\firestorm\\firestorm.dll"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "console": "externalTerminal"
    },
    {
      "type": "windbg",
      "request": "launch",
      "name": "Launch Program",
      "target": ".\\frag.exe",
      "verbosity": "debug",
      "windbgpath": "C:/Program Files (x86)/Windows Kits/10/Debuggers/x64/dbgeng.dll",
      "workingDir": "${workspaceRoot}",
      "args": "-r ..\\firestorm\\firestorm.dll",
    }
    
  ]
}